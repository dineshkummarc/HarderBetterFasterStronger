<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="HarderBetterFasterStronger" namespace="HarderBetterFasterStronger.Models">
    <class name="Activity">
        <id name="Id" generator="identity" />
        <property name="Name" />
        <property name="Weight" />
        <property name="IsComplete" />
        <property name="IsFailure" />
        <property name="Date" />
        <many-to-one name="Workout" column="WorkoutId" not-null="true" />
    </class>
</hibernate-mapping>